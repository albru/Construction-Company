      .most_ogr1.most_ogr1_wrap.section.section-margin
        .container.catalog-cont 
          .row
            .col-xs-12
              h1.catalog_title Мостовые односторонние ограждения барьерного типа
          .row
            .col-xs-12
              - var most_ogr1 = [{desc: 'Мостовое одностороннее ограждение усиленное двумя балками с верху 11МОУ2Бв с У8-У10'},{desc:'Мостовое одностороннее 3N ограждение усиленное балкой 11МОтБв У6 - У9'},{desc:'Мостовое односторонее ограждение усиленное балкой 11МОУБв У4 -У10'},{desc:'Мостовое односторонее 3N ограждение 11МОт У4 -У6'},{desc:'Мостовое одностороннее 3N ограждение с верхним прогоном 11МОтв У6 - У10'},{desc:'Мостовое дностороннее 3N ограждение с нижним прогоном 11МОтн У5 - У8'},{desc:'Мостовое одностороннее ограждение усиленное трубой 11МОУТ У4 - У8'},{desc:'Мостовое одностороннее ограждение с верхним прогоном 11МОв'},{desc:'Мостовое одностороннее ограждение 11МО с У1 - У5'}]
              .row
                each val, index in most_ogr1
                  .col-lg-3.col-xs-12.catalog-card_wrap
                    .card.catalog-card
                      .card-body.catalog-card_body-cont 
                        img.catalog-card_img(src=`../../../img/catalog/mostovie_ograzhdeniya/most_ogr_1/${index + 1}.jpg`)
                      .card-footer.catalog-card_footer-cont= `${val.desc}`
    script(src="../../../js/libs.min.js") 
    script(src="../../../js/bundle.js")